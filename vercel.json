{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
    },
    "functions": {
        "api/*.js": {
            "memory": 3008,
            "maxDuration": 60
        }
    }
} 